{
  "mcpServers": {
    "balance-advisor": {
      "command": "node",
      "args": ["mcp_advisor/dist/index.js"],
      "env": {
        "DAEMON_URL": "http://localhost:3001",
        "CONTENT_DIR": "./content"
      }
    },
    "playwright": {
      "command": "npx",
      "args": ["tsx", "e2e/mcp-server.ts"],
      "env": {
        "BASE_URL": "http://localhost:5173"
      }
    }
  }
}
